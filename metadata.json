{
  "name": "ikonia-enviroplus",
  "version": "1.0.0",
  "author": "ikonia",
  "summary": "enviro+ platform",
  "license": "GPL-3.0+",
  "source": "https://github.com/ikonia/ikonia-enviroplus",
  "project_page": "https://github.com/ikonia/puppet-puppet",
  "issues_url": "https://github.com/konia/ikonia-enviroplus/issues",
  "description": "Module for installing enviro+ and data exporters",
